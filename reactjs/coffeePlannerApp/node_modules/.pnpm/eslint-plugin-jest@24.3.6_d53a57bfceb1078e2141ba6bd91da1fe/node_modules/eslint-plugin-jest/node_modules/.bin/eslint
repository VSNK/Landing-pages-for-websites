#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/reactjs/coding-practices/coffeePlannerApp/node_modules/.pnpm/eslint@7.28.0/node_modules/eslint/bin/eslint.js/node_modules:/home/workspace/reactjs/coding-practices/coffeePlannerApp/node_modules/.pnpm/eslint@7.28.0/node_modules/eslint/bin/node_modules:/home/workspace/reactjs/coding-practices/coffeePlannerApp/node_modules/.pnpm/eslint@7.28.0/node_modules/eslint/node_modules:/home/workspace/reactjs/coding-practices/coffeePlannerApp/node_modules/.pnpm/eslint@7.28.0/node_modules:/home/workspace/reactjs/coding-practices/coffeePlannerApp/node_modules/.pnpm/node_modules:/home/workspace/reactjs/coding-practices/coffeePlannerApp/node_modules:/home/workspace/reactjs/coding-practices/node_modules:/home/workspace/reactjs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/reactjs/coding-practices/coffeePlannerApp/node_modules/.pnpm/eslint-plugin-jest@24.3.6_d53a57bfceb1078e2141ba6bd91da1fe/node_modules/eslint-plugin-jest/node_modules:/home/workspace/reactjs/coding-practices/coffeePlannerApp/node_modules/.pnpm/eslint-plugin-jest@24.3.6_d53a57bfceb1078e2141ba6bd91da1fe/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../eslint@7.28.0/node_modules/eslint/bin/eslint.js" "$@"
else 
  exec node  "$basedir/../../../../../eslint@7.28.0/node_modules/eslint/bin/eslint.js" "$@"
fi
